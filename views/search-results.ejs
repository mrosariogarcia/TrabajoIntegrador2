<%- include("./partials/head") %>

    <body>
	<!-- HEADER -->
        <%- include("./partials/headerNoLogueado") %>

	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Resultados de b√∫squeda</h2>
			</div>
			<!-- Show the "ultimos" products -->
			<div class="container products-wrapper">
				<div class="row">
					<% for (let i = 0; i < 4; i++) { %>
						<div class="col-12 col-sm-6 col-lg-3">
							<section class="product-box">
								<a href="/product">
									<figure class="product-box_image">
										<img src="<%= info[i].imagen %>" alt="<%= info[i].producto %>">
									</figure>
									<article class="product-box_data">
										<h2><%= info[i].producto %></h2>
										<p><%= info[i].descripcion %></p>
										<p><%= info[i].comentarios %></p>
									</article>
								</a>
							</section>
						</div>						
					<% } %>
				</div>
			</div>
			

	<!-- FOOTER -->
		<%- include("./partials/footer") %>

</body>

</html>